        <field name="model_id" ref="model_kids_child_subscription"/>
        <field name="binding_model_id" ref="model_kids_child_subscription"/>
        <field name="binding_view_types">list</field>
        <field name="state">code</field>
        <field name="code">
if records:
    action = model.action_bulk_create_pos_orders(records.ids)
    if action:
        action = action
        </field>
    </record>
</odoo>
